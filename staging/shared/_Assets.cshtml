@inherits Custom.Hybrid.Razor12
@using ToSic.Razor.Blade;
@{
  var Edition = (Edit.Enabled ? "staging" : "live");
  var page = GetService<ToSic.Sxc.Web.IPageService>();  // Service to set titles etc. on the page
  page.Activate("turnOn");
  var mobiusDomId = "app-mobius5-" + @CmsContext.Module.Id;
}

<turnOn turn-on='{ "run": "window.appMobius5.init()", "data": { "domId": "@mobiusDomId" } }'></turnOn>
<script src="@App.Path/@Edition/dist/scripts.min.js" data-enableoptimizations="true"></script>
<link rel="stylesheet" type="text/css" href="@App.Path/@Edition/dist/styles.min.css" data-enableoptimizations="true">
